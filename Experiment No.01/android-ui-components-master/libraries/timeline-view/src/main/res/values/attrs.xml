<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimelineView">
        <attr name="tlv_dotSize" format="dimension" />
        <attr name="tlv_borderWidth" format="dimension" />
        <attr name="tlv_textSize" format="dimension" />
        <attr name="tlv_lineWidth" format="dimension" />
        <attr name="android:fontFamily"  format="string"/>
        <attr name="tlv_orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>